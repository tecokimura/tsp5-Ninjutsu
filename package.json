{
    "private": true,
    "scripts": {
        "dev": "run-s dev:*",
        "dev:prettier": "prettier --write src/**/*.ts src/**/**/*.ts __tests__/**/*.ts",
        "dev:test": "jest",
        "dev:parcel": "parcel src/index.html",
        "build": "run-s build:*",
        "build:prettier": "prettier --write src/**/*.ts src/**/**/*.ts __tests__/**/*.ts",
        "build:test": "jest",
        "build:build": "parcel build src/index.html",
        "ver": "run-s ver:*",
        "ver:prettier": "prettier --version",
        "ver:jest": "jest --version",
        "ver:parcel": "parcel --version"
    },
    "devDependencies": {
        "@types/jest": "^23.3.10",
        "@types/p5": "^1.4.0",
        "jest": "^29.2.1",
        "npm-run-all": "^4.1.5",
        "p5": "^1.4.0",
        "parcel-bundler": "^1.10.3",
        "prettier": "^2.3.0",
        "ts-jest": "^29.0.3"
    },
    "dependencies": {
        "@types/p5": "^1.4.0",
        "p5": "^1.4.0",
        "parcel-bundler": "^1.10.3"
    },
    "name": "tsp5-ninjutsu",
    "description": "Ninja in html5 game",
    "version": "1.0.0",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/tecokimura/tsp5-Ninjutsu.git"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/tecokimura/tsp5-Ninjutsu/issues"
    },
    "homepage": "https://github.com/tecokimura/tsp5-Ninjutsu#readme",
    "jest": {
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js"
        ],
        "transform": {
            "^.+\\.(ts|tsx)$": "ts-jest"
        },
        "testMatch": [
            "**/__tests__/*.+(ts|tsx|js)",
            "**/__tests__/**/*.+(ts|tsx|js)"
        ]
    }
}
